{
    "name" : "MyCEL" ,
    "target_type" : "library",
    "parameters" : [
        "STATIC"
    ],
    "sources" : [
        "$<TARGET_OBJECTS:MyCEL::utils>",
        "$<TARGET_OBJECTS:MyCEL::basics>",
        "$<TARGET_OBJECTS:MyCEL::math>"
    ],
    "include_directories" : {
        "private" : [ 
            "include"
        ],
        "interface" : [ 
            "$<BUILD_INTERFACE:${CMAKE_CURRENT_SOURCE_DIR}/include>",
            "$<INSTALL_INTERFACE:include>" ]
    },
    "compile_features" : {
        "public" : [
            "cxx_std_20"
        ]
    },
    "link_libraries" : {
        "private": [
            "MyCEL::stdext",
            "MyCEL::intrin",
            "MyCEL::simd_rand",
            "MyCEL::types"
        ]
    },
    "properties" : [{
        "name": "LINKER_LANGUAGE",
        "value": "CXX"
    }],
    "public_include_directory" : "include/"
}
